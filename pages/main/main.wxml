<view class='device'>
  <view class='header'></view>
  <view style='margin-top: 120rpx'>
  <scroll-view scroll-y style="height:{{winHeight - 100}}px">
  <view class='content'>
    <view wx:for="{{items}}" wx:key="items" class='box' bindtap='navigateToDetail' data-desc='{{item.desc}}' >
      <image src='/images/icons/naruto.jpg' class='box_image'></image>
      <view class='box_title'>{{item.title}}</view>
    </view>
  </view>
  </scroll-view>
  </view>
</view>